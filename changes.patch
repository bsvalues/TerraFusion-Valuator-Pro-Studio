-/// Calculate weight for each comparable based on multiple factors
-fn calculate_comp_weight(comp: &ComparableSale, similarity_score: f64) -> f64 {
-    let mut weight = 1.0;
-
-    // Weight by similarity (higher similarity = higher weight)
-    weight *= similarity_score;
-
-    // Weight by proximity (closer = higher weight)
-    if let Some(distance) = get_comp_distance(comp) {
-        let proximity_weight = if distance <= 0.25 {
-            1.0 // Very close
-        } else if distance <= 0.5 {
-            0.9 // Close
-        } else if distance <= 1.0 {
-            0.8 // Moderate
-        } else {
-            0.6 // Distant but usable
-        };
-        weight *= proximity_weight;
-    }
-
-    // Weight by recency (more recent = higher weight)
-    // Note: In real implementation, this would parse sale_date
-    // For now, assume all sales are reasonably recent
-
-    weight.max(0.1) // Minimum weight to avoid zero weights
