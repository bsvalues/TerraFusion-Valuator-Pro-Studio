-use crate::db::Db;\nuse crate::types::*;\nuse crate::rules::QcRuleEngine;\nuse crate::ai_integration::RiskSentinelAI;\nuse axum::{\n    extract::{Path, State},\n    http::StatusCode,\n    response::{IntoResponse, Json},\n    routing::{get, post},\n    Router,\n};\nuse uuid::Uuid;\nuse std::time::Instant;\n\npub fn qc_routes() -> Router<Db> {\n    Router::new()\n        .route(\"/analyze/:order_id\", post(run_qc_analysis))\n        .route(\"/status/:order_id\", get(get_qc_status))\n        .route(\"/issues/:order_id\", get(get_qc_issues))\n        .route(\"/resolve/:issue_id\", post(resolve_qc_issue))\n        .route(\"/rules\", get(list_qc_rules))\n        .route(\"/dashboard\", get(qc_dashboard))\n}\n\n/// Run QC analysis for an order\nasync fn run_qc_analysis(\n    State(db): State<Db>,\n    Path(order_id): Path<Uuid>,\n    Json(request): Json<RunQcAnalysisRequest>,\n) -> Result<impl IntoResponse, impl IntoResponse> {\n    let start_time = Instant::now();\n    \n    // Create QC analysis session\n    let session_id = Uuid::new_v4();\n    let session_type = request.session_type.unwrap_or(QcSessionType::FullAnalysis);\n    let triggered_by = request.triggered_by.unwrap_or_else(|| \"api\".to_string());\n    \n    // Insert analysis session\n    let session_result = sqlx::query(\n        r#\"\n        INSERT INTO qc_analysis_sessions \n            (id, order_id, session_type, trigger_event, triggered_by, status)\n        VALUES ($1, $2, $3, 'manual_api', $4, 'running')\n        RETURNING id\n        \"#\n    )\n    .bind(session_id)\n    .bind(order_id)\n    .bind(session_type_to_string(&session_type))\n    .bind(&triggered_by)\n    .fetch_one(&db)\n    .await;\n    \n    if let Err(e) = session_result {\n        tracing::error!(\"Failed to create QC session: {}\", e);\n        return Err((\n            StatusCode::INTERNAL_SERVER_ERROR,\n            Json(serde_json::json!({\n                \"error\": \"Failed to create QC analysis session\",\n                \"details\": e.to_string()\n            }))\n        ));\n    }\n    \n    // Initialize QC engine\n    let qc_engine = QcRuleEngine::new(db.clone());\n    let ai_engine = RiskSentinelAI::new();\n    \n    // Get order data\n    let order_data = match qc_engine.get_order_data(order_id).await {\n        Ok(data) => data,\n        Err(e) => {\n            tracing::error!(\"Failed to get order data: {}\", e);\n            return Err((\n                StatusCode::NOT_FOUND,\n                Json(serde_json::json!({\n                    \"error\": \"Order not found or unable to load data\",\n                    \"order_id\": order_id\n                }))\n            ));\n        }\n    };\n    \n    // Get active rules\n    let rules = match qc_engine.get_active_rules().await {\n        Ok(rules) => rules,\n        Err(e) => {\n            tracing::error!(\"Failed to get QC rules: {}\", e);\n            return Err((\n                StatusCode::INTERNAL_SERVER_ERROR,\n                Json(serde_json::json!({\n                    \"error\": \"Failed to load QC rules\",\n                    \"details\": e.to_string()\n                }))\n            ));\n        }\n    };\n    \n    // Execute rules\n    let mut rules_passed = 0;\n    let mut rules_failed = 0;\n    let mut rules_warning = 0;\n    let mut rules_error = 0;\n    let mut ai_interactions = 0;\n    let mut total_ai_tokens = 0;\n    \n    for rule in &rules {\n        match qc_engine.execute_rule(rule, &order_data).await {\n            Ok(execution) => {\n                match execution.execution_status {\n                    QcExecutionStatus::Passed => rules_passed += 1,\n                    QcExecutionStatus::Failed => {\n                        rules_failed += 1;\n                        // Create QC issue for failed rule\n                        let _ = create_qc_issue_from_execution(&db, &execution, rule).await;\n                    },\n                    QcExecutionStatus::Warning => rules_warning += 1,\n                    QcExecutionStatus::Error => rules_error += 1,\n                    QcExecutionStatus::PendingAi => {\n                        // Handle AI analysis if requested\n                        if request.include_ai_analysis.unwrap_or(false) && rule.requires_ai_analysis {\n                            if let Some(prompt_template) = &rule.ai_prompt_template {\n                                match ai_engine.analyze_comparable_relevance(&order_data, prompt_template).await {\n                                    Ok(ai_interaction) => {\n                                        ai_interactions += 1;\n                                        total_ai_tokens += ai_interaction.tokens_received.unwrap_or(0);\n                                        // Store AI interaction would go here\n                                    },\n                                    Err(e) => {\n                                        tracing::warn!(\"AI analysis failed for rule {}: {}\", rule.rule_code, e);\n                                    }\n                                }\n                            }\n                        }\n                        rules_passed += 1; // Count as passed for now\n                    }\n                }\n            },\n            Err(e) => {\n                tracing::error!(\"Rule execution failed for {}: {}\", rule.rule_code, e);\n                rules_error += 1;\n            }\n        }\n    }\n    \n    let total_rules = rules.len() as i32;\n    let execution_time_ms = start_time.elapsed().as_millis() as i32;\n    \n    // Calculate risk score (simplified algorithm)\n    let risk_score = calculate_risk_score(rules_failed, rules_warning, total_rules);\n    let risk_level = risk_score_to_level(risk_score);\n    \n    // Update session with results\n    let update_result = sqlx::query(\n        r#\"\n        UPDATE qc_analysis_sessions SET\n            total_rules_executed = $1,\n            rules_passed = $2,\n            rules_failed = $3,\n            rules_warning = $4,\n            rules_error = $5,\n            overall_risk_score = $6,\n            risk_level = $7,\n            execution_duration_ms = $8,\n            ai_requests_made = $9,\n            ai_tokens_consumed = $10,\n            status = 'completed',\n            completion_percentage = 100,\n            completed_at = now()\n        WHERE id = $11\n        \"#\n    )\n    .bind(total_rules)\n    .bind(rules_passed)\n    .bind(rules_failed)\n    .bind(rules_warning)\n    .bind(rules_error)\n    .bind(risk_score)\n    .bind(risk_level_to_string(&risk_level))\n    .bind(execution_time_ms)\n    .bind(ai_interactions)\n    .bind(total_ai_tokens)\n    .bind(session_id)\n    .execute(&db)\n    .await;\n    \n    if let Err(e) = update_result {\n        tracing::error!(\"Failed to update QC session: {}\", e);\n    }\n    \n    let response = QcAnalysisResponse {\n        session_id,\n        order_id,\n        status: QcSessionStatus::Completed,\n        risk_level: risk_level.clone(),\n        overall_risk_score: risk_score,\n        total_issues: rules_failed,\n        critical_issues: 0, // Would need to count by severity\n        high_issues: rules_failed, // Simplified\n        execution_summary: QcExecutionSummary {\n            rules_executed: total_rules,\n            rules_passed,\n            rules_failed,\n            rules_warning,\n            ai_interactions,\n            execution_time_ms,\n            completion_percentage: 100,\n        },\n    };\n    \n    Ok((StatusCode::OK, Json(response)))\n}\n\n/// Get QC status for an order\nasync fn get_qc_status(\n    State(db): State<Db>,\n    Path(order_id): Path<Uuid>,\n) -> Result<impl IntoResponse, impl IntoResponse> {\n    let result = sqlx::query(\n        r#\"\n        SELECT qs.*, o.client_name, sp.address\n        FROM qc_analysis_sessions qs\n        LEFT JOIN orders o ON qs.order_id = o.id\n        LEFT JOIN subject_properties sp ON o.id = sp.order_id\n        WHERE qs.order_id = $1\n        ORDER BY qs.started_at DESC\n        LIMIT 1\n        \"#\n    )\n    .bind(order_id)\n    .fetch_optional(&db)\n    .await;\n    \n    match result {\n        Ok(Some(row)) => {\n            // Count issues\n            let issue_counts = sqlx::query(\n                r#\"\n                SELECT \n                    COUNT(*) as total_issues,\n                    COUNT(CASE WHEN severity = 'critical' THEN 1 END) as critical_issues,\n                    COUNT(CASE WHEN severity = 'high' THEN 1 END) as high_issues,\n                    COUNT(CASE WHEN status = 'open' THEN 1 END) as open_issues\n                FROM qc_issues \n                WHERE order_id = $1\n                \"#\n            )\n            .bind(order_id)\n            .fetch_one(&db)\n            .await;\n            \n            let (total_issues, critical_issues, high_issues, open_issues) = match issue_counts {\n                Ok(counts) => (\n                    counts.get::<i64, _>(\"total_issues\") as i32,\n                    counts.get::<i64, _>(\"critical_issues\") as i32,\n                    counts.get::<i64, _>(\"high_issues\") as i32,\n                    counts.get::<i64, _>(\"open_issues\") as i32,\n                ),\n                Err(_) => (0, 0, 0, 0),\n            };\n            \n            let summary = QcOrderSummary {\n                order_id,\n                client_name: row.get(\"client_name\"),\n                property_address: row.get::<Option<String>, _>(\"address\").unwrap_or_else(|| \"Unknown\".to_string()),\n                latest_session: None, // Would populate with session data\n                total_issues,\n                critical_issues,\n                high_issues,\n                open_issues,\n                risk_level: QcRiskLevel::Unknown, // Would parse from string\n                last_qc_run: row.get(\"completed_at\"),\n            };\n            \n            Ok((StatusCode::OK, Json(summary)))\n        },\n        Ok(None) => Err((\n            StatusCode::NOT_FOUND,\n            Json(serde_json::json!({\n                \"error\": \"No QC analysis found for this order\",\n                \"order_id\": order_id\n            }))\n        )),\n        Err(e) => {\n            tracing::error!(\"Failed to get QC status: {}\", e);\n            Err((\n                StatusCode::INTERNAL_SERVER_ERROR,\n                Json(serde_json::json!({\n                    \"error\": \"Database error\",\n                    \"details\": e.to_string()\n                }))\n            ))\n        }\n    }\n}\n\n/// Get QC issues for an order\nasync fn get_qc_issues(\n    State(db): State<Db>,\n    Path(order_id): Path<Uuid>,\n) -> Result<impl IntoResponse, impl IntoResponse> {\n    let result = sqlx::query(\n        r#\"\n        SELECT id, order_id, severity, category, code, title, message, \n               status, risk_score, auto_generated, requires_human_review,\n               created_at, updated_at\n        FROM qc_issues \n        WHERE order_id = $1\n        ORDER BY risk_score DESC, created_at DESC\n        \"#\n    )\n    .bind(order_id)\n    .fetch_all(&db)\n    .await;\n    \n    match result {\n        Ok(rows) => {\n            let issues: Vec<serde_json::Value> = rows.iter().map(|row| {\n                serde_json::json!({\n                    \"id\": row.get::<Uuid, _>(\"id\"),\n                    \"order_id\": row.get::<Uuid, _>(\"order_id\"),\n                    \"severity\": row.get::<String, _>(\"severity\"),\n                    \"category\": row.get::<String, _>(\"category\"),\n                    \"code\": row.get::<String, _>(\"code\"),\n                    \"title\": row.get::<String, _>(\"title\"),\n                    \"message\": row.get::<String, _>(\"message\"),\n                    \"status\": row.get::<String, _>(\"status\"),\n                    \"risk_score\": row.get::<i32, _>(\"risk_score\"),\n                    \"auto_generated\": row.get::<bool, _>(\"auto_generated\"),\n                    \"requires_human_review\": row.get::<bool, _>(\"requires_human_review\"),\n                    \"created_at\": row.get::<chrono::DateTime<chrono::Utc>, _>(\"created_at\"),\n                    \"updated_at\": row.get::<chrono::DateTime<chrono::Utc>, _>(\"updated_at\")\n                })\n            }).collect();\n            \n            Ok((StatusCode::OK, Json(issues)))\n        },\n        Err(e) => {\n            tracing::error!(\"Failed to get QC issues: {}\", e);\n            Err((\n                StatusCode::INTERNAL_SERVER_ERROR,\n                Json(serde_json::json!({\n                    \"error\": \"Database error\",\n                    \"details\": e.to_string()\n                }))\n            ))\n        }\n    }\n}\n\n/// Resolve a QC issue\nasync fn resolve_qc_issue(\n    State(db): State<Db>,\n    Path(issue_id): Path<Uuid>,\n    Json(request): Json<ResolveQcIssueRequest>,\n) -> Result<impl IntoResponse, impl IntoResponse> {\n    let result = sqlx::query(\n        r#\"\n        UPDATE qc_issues SET\n            status = $1,\n            resolution_notes = $2,\n            resolved_at = now(),\n            updated_at = now()\n        WHERE id = $3\n        RETURNING id\n        \"#\n    )\n    .bind(qc_issue_status_to_string(&request.new_status))\n    .bind(&request.resolution_notes)\n    .bind(issue_id)\n    .fetch_optional(&db)\n    .await;\n    \n    match result {\n        Ok(Some(_)) => Ok((\n            StatusCode::OK,\n            Json(serde_json::json!({\n                \"message\": \"QC issue resolved successfully\",\n                \"issue_id\": issue_id\n            }))\n        )),\n        Ok(None) => Err((\n            StatusCode::NOT_FOUND,\n            Json(serde_json::json!({\n                \"error\": \"QC issue not found\",\n                \"issue_id\": issue_id\n            }))\n        )),\n        Err(e) => {\n            tracing::error!(\"Failed to resolve QC issue: {}\", e);\n            Err((\n                StatusCode::INTERNAL_SERVER_ERROR,\n                Json(serde_json::json!({\n                    \"error\": \"Database error\",\n                    \"details\": e.to_string()\n                }))\n            ))\n        }\n    }\n}\n\n/// List available QC rules\nasync fn list_qc_rules(\n    State(db): State<Db>,\n) -> Result<impl IntoResponse, impl IntoResponse> {\n    let result = sqlx::query(\n        r#\"\n        SELECT rule_code, rule_name, category, severity_level, description,\n               rule_type, requires_ai_analysis, is_active\n        FROM qc_rule_definitions\n        ORDER BY category, severity_level, rule_code\n        \"#\n    )\n    .fetch_all(&db)\n    .await;\n    \n    match result {\n        Ok(rows) => {\n            let rules: Vec<serde_json::Value> = rows.iter().map(|row| {\n                serde_json::json!({\n                    \"rule_code\": row.get::<String, _>(\"rule_code\"),\n                    \"rule_name\": row.get::<String, _>(\"rule_name\"),\n                    \"category\": row.get::<String, _>(\"category\"),\n                    \"severity_level\": row.get::<String, _>(\"severity_level\"),\n                    \"description\": row.get::<String, _>(\"description\"),\n                    \"rule_type\": row.get::<String, _>(\"rule_type\"),\n                    \"requires_ai_analysis\": row.get::<bool, _>(\"requires_ai_analysis\"),\n                    \"is_active\": row.get::<bool, _>(\"is_active\")\n                })\n            }).collect();\n            \n            Ok((StatusCode::OK, Json(rules)))\n        },\n        Err(e) => {\n            tracing::error!(\"Failed to list QC rules: {}\", e);\n            Err((\n                StatusCode::INTERNAL_SERVER_ERROR,\n                Json(serde_json::json!({\n                    \"error\": \"Database error\",\n                    \"details\": e.to_string()\n                }))\n            ))\n        }\n    }\n}\n\n/// QC dashboard summary\nasync fn qc_dashboard(\n    State(db): State<Db>,\n) -> Result<impl IntoResponse, impl IntoResponse> {\n    // This would return dashboard statistics\n    // For now, return a placeholder\n    let dashboard = serde_json::json!({\n        \"total_orders_analyzed\": 0,\n        \"pending_reviews\": 0,\n        \"critical_issues\": 0,\n        \"ai_interactions_today\": 0,\n        \"status\": \"RiskSentinel operational\"\n    });\n    \n    Ok((StatusCode::OK, Json(dashboard)))\n}\n\n// Helper functions\n\nasync fn create_qc_issue_from_execution(\n    db: &Db,\n    execution: &QcRuleExecution,\n    rule: &QcRuleDefinition,\n) -> Result<Uuid, sqlx::Error> {\n    let issue_id = Uuid::new_v4();\n    \n    sqlx::query(\n        r#\"\n        INSERT INTO qc_issues (\n            id, order_id, rule_execution_id, severity, category, code,\n            title, message, status, risk_score, auto_generated\n        ) VALUES ($1, $2, $3, $4, $5, $6, $7, $8, 'open', $9, true)\n        \"#\n    )\n    .bind(issue_id)\n    .bind(execution.order_id)\n    .bind(execution.id)\n    .bind(qc_severity_to_string(&rule.severity_level))\n    .bind(qc_category_to_string(&rule.category))\n    .bind(&rule.rule_code)\n    .bind(&rule.rule_name)\n    .bind(execution.finding_summary.as_ref().unwrap_or(&\"QC rule failed\".to_string()))\n    .bind(50) // Default risk score\n    .execute(db)\n    .await?;\n    \n    Ok(issue_id)\n}\n\nfn calculate_risk_score(failed_rules: i32, warning_rules: i32, total_rules: i32) -> i32 {\n    if total_rules == 0 {\n        return 0;\n    }\n    \n    let failure_rate = (failed_rules as f64 / total_rules as f64) * 100.0;\n    let warning_rate = (warning_rules as f64 / total_rules as f64) * 100.0;\n    \n    let risk_score = (failure_rate * 0.8 + warning_rate * 0.2) as i32;\n    std::cmp::min(100, risk_score)\n}\n\nfn risk_score_to_level(score: i32) -> QcRiskLevel {\n    match score {\n        0..=25 => QcRiskLevel::Low,\n        26..=50 => QcRiskLevel::Medium,\n        51..=75 => QcRiskLevel::High,\n        76..=100 => QcRiskLevel::Critical,\n        _ => QcRiskLevel::Unknown,\n    }\n}\n\n// String conversion helpers (simplified)\nfn session_type_to_string(session_type: &QcSessionType) -> &'static str {\n    match session_type {\n        QcSessionType::FullAnalysis => \"full_analysis\",\n        QcSessionType::Incremental => \"incremental\",\n        QcSessionType::AiReview => \"ai_review\",\n        QcSessionType::ManualTrigger => \"manual_trigger\",\n    }\n}\n\nfn risk_level_to_string(risk_level: &QcRiskLevel) -> &'static str {\n    match risk_level {\n        QcRiskLevel::Low => \"low\",\n        QcRiskLevel::Medium => \"medium\",\n        QcRiskLevel::High => \"high\",\n        QcRiskLevel::Critical => \"critical\",\n        QcRiskLevel::Unknown => \"unknown\",\n    }\n}\n\nfn qc_severity_to_string(severity: &QcSeverity) -> &'static str {\n    match severity {\n        QcSeverity::Critical => \"critical\",\n        QcSeverity::High => \"high\",\n        QcSeverity::Medium => \"medium\",\n        QcSeverity::Low => \"low\",\n        QcSeverity::Info => \"info\",\n    }\n}\n\nfn qc_category_to_string(category: &QcCategory) -> &'static str {\n    match category {\n        QcCategory::DataQuality => \"data_quality\",\n        QcCategory::Compliance => \"compliance\",\n        QcCategory::MarketAnalysis => \"market_analysis\",\n        QcCategory::RiskAssessment => \"risk_assessment\",\n    }\n}\n\nfn qc_issue_status_to_string(status: &QcIssueStatus) -> &'static str {\n    match status {\n        QcIssueStatus::Open => \"open\",\n        QcIssueStatus::Resolved => \"resolved\",\n        QcIssueStatus::Ignored => \"ignored\",\n        QcIssueStatus::Deferred => \"deferred\",\n    }\n}"
