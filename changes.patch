-fn calculate_sales_reliability(sales: &ValuationResult) -> f64 {
-    let mut score: f64 = 0.7; // Base score
-
-    // Adjust for number of comps
-    if sales.comps.len() >= 5 {
-        score += 0.1;
-    } else if sales.comps.len() < 3 {
-        score -= 0.2;
-    }
-
-    // Adjust for average similarity
-    let avg_similarity: f64 =
-        sales.comps.iter().map(|c| c.similarity_score).sum::<f64>() / sales.comps.len() as f64;
-    score += (avg_similarity - 0.7) * 0.5;
-
-    // Adjust for adjustment levels
-    let avg_gross_adj: f64 = sales
-        .comps
-        .iter()
-        .map(|c| c.gross_adjustment_pct)
-        .sum::<f64>()
-        / sales.comps.len() as f64;
-    if avg_gross_adj > 25.0 {
-        score -= 0.2;
-    }
