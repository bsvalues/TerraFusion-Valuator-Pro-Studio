-/// Main reconciliation function that combines multiple valuation approaches
-pub fn reconcile(input: &ReconciliationInput) -> anyhow::Result<ReconciliationResult> {
-    let mut contributions: Vec<ApproachContribution> = Vec::new();
-    let mut total_weighted_value = 0.0;
-    let mut total_weight = 0.0;
-
-    // Sales Comparison Approach
-    if let Some(sales_result) = &input.sales {
-        if input.weight_sales > 0.0 {
-            let contribution = input.weight_sales * sales_result.reconciled_value;
-            contributions.push(ApproachContribution {
-                approach_name: "Sales Comparison".to_string(),
-                indicated_value: sales_result.reconciled_value,
-                weight_applied: input.weight_sales,
-                weighted_contribution: contribution,
-                reliability_factors: assess_sales_reliability(sales_result),
-            });
-            total_weighted_value += contribution;
-            total_weight += input.weight_sales;
-        }
-    }
-
-    // Cost Approach
-    if let Some(cost_result) = &input.cost {
-        if input.weight_cost > 0.0 {
-            let contribution = input.weight_cost * cost_result.indicated_value;
-            contributions.push(ApproachContribution {
-                approach_name: "Cost Approach".to_string(),
-                indicated_value: cost_result.indicated_value,
-                weight_applied: input.weight_cost,
-                weighted_contribution: contribution,
-                reliability_factors: assess_cost_reliability(cost_result),
-            });
-            total_weighted_value += contribution;
-            total_weight += input.weight_cost;
-        }
-    }
-
-    // Income Approach
-    if let Some(income_result) = &input.income {
-        if input.weight_income > 0.0 {
-            let contribution = input.weight_income * income_result.indicated_value;
-            contributions.push(ApproachContribution {
-                approach_name: "Income Approach".to_string(),
-                indicated_value: income_result.indicated_value,
-                weight_applied: input.weight_income,
-                weighted_contribution: contribution,
-                reliability_factors: assess_income_reliability(income_result),
-            });
-            total_weighted_value += contribution;
-            total_weight += input.weight_income;
-        }
-    }
-
-    if total_weight <= 0.0 {
-        return Err(anyhow::anyhow!(
-            "No valid approaches with positive weights provided for reconciliation"
-        ));
-    }
-
-    if contributions.is_empty() {
-        return Err(anyhow::anyhow!(
-            "At least one valuation approach result is required"
-        ));
