-/// Determine confidence level based on approach consistency
-fn determine_confidence_level(contributions: &[ApproachContribution]) -> ReconciliationConfidence {
-    if contributions.len() == 1 {
-        return ReconciliationConfidence::Medium;
-    }
-
-    // Calculate coefficient of variation among indicated values
-    let values: Vec<f64> = contributions.iter().map(|c| c.indicated_value).collect();
-    let mean = values.iter().sum::<f64>() / values.len() as f64;
-    let variance = values.iter().map(|v| (v - mean).powi(2)).sum::<f64>() / values.len() as f64;
-    let std_dev = variance.sqrt();
-    let cv = std_dev / mean;
-
-    if cv <= 0.05 {
-        ReconciliationConfidence::High // Within 5%
-    } else if cv <= 0.15 {
-        ReconciliationConfidence::Medium // Within 15%
-    } else {
-        ReconciliationConfidence::Low // Greater than 15% variation
-    }
-}
