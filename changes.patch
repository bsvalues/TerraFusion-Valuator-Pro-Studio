-use serde::{Deserialize, Serialize};\nuse uuid::Uuid;\nuse chrono::{DateTime, Utc};\n\n// =============================================================================\n// QC RULE TYPES\n// =============================================================================\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub struct QcRuleDefinition {\n    pub id: Uuid,\n    pub rule_code: String,\n    pub rule_name: String,\n    pub category: QcCategory,\n    pub severity_level: QcSeverity,\n    pub description: String,\n    pub rule_type: QcRuleType,\n    pub field_path: Option<String>,\n    pub validation_logic: serde_json::Value,\n    pub requires_ai_analysis: bool,\n    pub ai_prompt_template: Option<String>,\n    pub is_active: bool,\n    pub auto_resolve: bool,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcCategory {\n    #[serde(rename = \"data_quality\")]\n    DataQuality,\n    #[serde(rename = \"compliance\")]\n    Compliance,\n    #[serde(rename = \"market_analysis\")]\n    MarketAnalysis,\n    #[serde(rename = \"risk_assessment\")]\n    RiskAssessment,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcSeverity {\n    #[serde(rename = \"critical\")]\n    Critical,\n    #[serde(rename = \"high\")]\n    High,\n    #[serde(rename = \"medium\")]\n    Medium,\n    #[serde(rename = \"low\")]\n    Low,\n    #[serde(rename = \"info\")]\n    Info,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcRuleType {\n    #[serde(rename = \"field_validation\")]\n    FieldValidation,\n    #[serde(rename = \"comparison_check\")]\n    ComparisonCheck,\n    #[serde(rename = \"ai_assisted\")]\n    AiAssisted,\n    #[serde(rename = \"statistical_analysis\")]\n    StatisticalAnalysis,\n}\n\n// =============================================================================\n// QC EXECUTION TYPES\n// =============================================================================\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct QcRuleExecution {\n    pub id: Uuid,\n    pub order_id: Uuid,\n    pub rule_id: Uuid,\n    pub execution_status: QcExecutionStatus,\n    pub execution_time_ms: Option<i32>,\n    pub finding_summary: Option<String>,\n    pub detailed_analysis: Option<String>,\n    pub confidence_score: Option<f64>,\n    pub evaluated_data: serde_json::Value,\n    pub rule_parameters: serde_json::Value,\n    pub ai_request_id: Option<String>,\n    pub ai_response_raw: Option<String>,\n    pub ai_tokens_used: Option<i32>,\n    pub executed_at: DateTime<Utc>,\n    pub executed_by: String,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcExecutionStatus {\n    #[serde(rename = \"passed\")]\n    Passed,\n    #[serde(rename = \"failed\")]\n    Failed,\n    #[serde(rename = \"warning\")]\n    Warning,\n    #[serde(rename = \"error\")]\n    Error,\n    #[serde(rename = \"pending_ai\")]\n    PendingAi,\n}\n\n// =============================================================================\n// QC ANALYSIS SESSION TYPES\n// =============================================================================\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct QcAnalysisSession {\n    pub id: Uuid,\n    pub order_id: Uuid,\n    pub session_type: QcSessionType,\n    pub trigger_event: String,\n    pub total_rules_executed: i32,\n    pub rules_passed: i32,\n    pub rules_failed: i32,\n    pub rules_warning: i32,\n    pub rules_error: i32,\n    pub overall_risk_score: i32,\n    pub risk_level: QcRiskLevel,\n    pub execution_duration_ms: Option<i32>,\n    pub ai_requests_made: i32,\n    pub ai_tokens_consumed: i32,\n    pub status: QcSessionStatus,\n    pub completion_percentage: i32,\n    pub started_at: DateTime<Utc>,\n    pub completed_at: Option<DateTime<Utc>>,\n    pub triggered_by: String,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcSessionType {\n    #[serde(rename = \"full_analysis\")]\n    FullAnalysis,\n    #[serde(rename = \"incremental\")]\n    Incremental,\n    #[serde(rename = \"ai_review\")]\n    AiReview,\n    #[serde(rename = \"manual_trigger\")]\n    ManualTrigger,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcSessionStatus {\n    #[serde(rename = \"running\")]\n    Running,\n    #[serde(rename = \"completed\")]\n    Completed,\n    #[serde(rename = \"failed\")]\n    Failed,\n    #[serde(rename = \"cancelled\")]\n    Cancelled,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcRiskLevel {\n    #[serde(rename = \"low\")]\n    Low,\n    #[serde(rename = \"medium\")]\n    Medium,\n    #[serde(rename = \"high\")]\n    High,\n    #[serde(rename = \"critical\")]\n    Critical,\n    #[serde(rename = \"unknown\")]\n    Unknown,\n}\n\n// =============================================================================\n// QC ISSUE TYPES (Enhanced)\n// =============================================================================\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct QcIssue {\n    pub id: Uuid,\n    pub order_id: Uuid,\n    pub rule_execution_id: Option<Uuid>,\n    pub severity: QcSeverity,\n    pub category: String,\n    pub code: String,\n    pub title: String,\n    pub message: String,\n    pub affected_field: Option<String>,\n    pub expected_value: Option<String>,\n    pub actual_value: Option<String>,\n    pub status: QcIssueStatus,\n    pub resolution_notes: Option<String>,\n    pub resolved_by: Option<Uuid>,\n    pub resolved_at: Option<DateTime<Utc>>,\n    pub metadata_json: Option<serde_json::Value>,\n    pub risk_score: i32,\n    pub auto_generated: bool,\n    pub ai_suggested_resolution: Option<String>,\n    pub impact_assessment: Option<String>,\n    pub requires_human_review: bool,\n    pub created_at: DateTime<Utc>,\n    pub updated_at: DateTime<Utc>,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum QcIssueStatus {\n    #[serde(rename = \"open\")]\n    Open,\n    #[serde(rename = \"resolved\")]\n    Resolved,\n    #[serde(rename = \"ignored\")]\n    Ignored,\n    #[serde(rename = \"deferred\")]\n    Deferred,\n}\n\n// =============================================================================\n// AI AGENT INTERACTION TYPES\n// =============================================================================\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct AiAgentInteraction {\n    pub id: Uuid,\n    pub order_id: Uuid,\n    pub qc_session_id: Option<Uuid>,\n    pub agent_name: String,\n    pub interaction_type: String,\n    pub request_prompt: String,\n    pub response_content: Option<String>,\n    pub model_used: Option<String>,\n    pub tokens_requested: Option<i32>,\n    pub tokens_received: Option<i32>,\n    pub cost_estimate: Option<f64>,\n    pub response_time_ms: Option<i32>,\n    pub confidence_score: Option<f64>,\n    pub status: AiInteractionStatus,\n    pub error_message: Option<String>,\n    pub requested_at: DateTime<Utc>,\n    pub completed_at: Option<DateTime<Utc>>,\n}\n\n#[derive(Debug, Clone, Serialize, Deserialize)]\npub enum AiInteractionStatus {\n    #[serde(rename = \"pending\")]\n    Pending,\n    #[serde(rename = \"completed\")]\n    Completed,\n    #[serde(rename = \"failed\")]\n    Failed,\n    #[serde(rename = \"timeout\")]\n    Timeout,\n}\n\n// =============================================================================\n// API REQUEST/RESPONSE TYPES\n// =============================================================================\n\n#[derive(Debug, Deserialize)]\npub struct RunQcAnalysisRequest {\n    pub order_id: Uuid,\n    pub session_type: Option<QcSessionType>,\n    pub rule_categories: Option<Vec<QcCategory>>,\n    pub include_ai_analysis: Option<bool>,\n    pub triggered_by: Option<String>,\n}\n\n#[derive(Debug, Serialize)]\npub struct QcAnalysisResponse {\n    pub session_id: Uuid,\n    pub order_id: Uuid,\n    pub status: QcSessionStatus,\n    pub risk_level: QcRiskLevel,\n    pub overall_risk_score: i32,\n    pub total_issues: i32,\n    pub critical_issues: i32,\n    pub high_issues: i32,\n    pub execution_summary: QcExecutionSummary,\n}\n\n#[derive(Debug, Serialize)]\npub struct QcExecutionSummary {\n    pub rules_executed: i32,\n    pub rules_passed: i32,\n    pub rules_failed: i32,\n    pub rules_warning: i32,\n    pub ai_interactions: i32,\n    pub execution_time_ms: i32,\n    pub completion_percentage: i32,\n}\n\n#[derive(Debug, Serialize)]\npub struct QcOrderSummary {\n    pub order_id: Uuid,\n    pub client_name: String,\n    pub property_address: String,\n    pub latest_session: Option<QcAnalysisSession>,\n    pub total_issues: i32,\n    pub critical_issues: i32,\n    pub high_issues: i32,\n    pub open_issues: i32,\n    pub risk_level: QcRiskLevel,\n    pub last_qc_run: Option<DateTime<Utc>>,\n}\n\n#[derive(Debug, Deserialize)]\npub struct ResolveQcIssueRequest {\n    pub issue_id: Uuid,\n    pub resolution_notes: String,\n    pub new_status: QcIssueStatus,\n}\n\n#[derive(Debug, Serialize)]\npub struct QcRulePerformance {\n    pub rule_code: String,\n    pub rule_name: String,\n    pub category: QcCategory,\n    pub severity_level: QcSeverity,\n    pub total_executions: i32,\n    pub failure_count: i32,\n    pub warning_count: i32,\n    pub avg_execution_time_ms: Option<f64>,\n    pub avg_confidence_score: Option<f64>,\n    pub ai_requests: i32,\n    pub avg_ai_tokens: Option<f64>,\n    pub last_executed: Option<DateTime<Utc>>,\n}\n\n// =============================================================================\n// ORDER DATA CONTEXT (for QC analysis)\n// =============================================================================\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct OrderDataContext {\n    pub order: OrderInfo,\n    pub subject_property: Option<SubjectPropertyInfo>,\n    pub comparables: Vec<ComparableInfo>,\n    pub valuation: Option<ValuationInfo>,\n}\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct OrderInfo {\n    pub id: Uuid,\n    pub client_name: String,\n    pub assignment_type: String,\n    pub status: String,\n    pub due_date: Option<chrono::NaiveDate>,\n    pub loan_amount: Option<f64>,\n}\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct SubjectPropertyInfo {\n    pub id: Uuid,\n    pub address: String,\n    pub city: Option<String>,\n    pub state: Option<String>,\n    pub gla_sqft: Option<f64>,\n    pub beds: Option<i16>,\n    pub baths: Option<f64>,\n    pub lot_sqft: Option<f64>,\n    pub year_built: Option<i32>,\n    pub quality: Option<String>,\n    pub condition: Option<String>,\n}\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct ComparableInfo {\n    pub id: Uuid,\n    pub address: String,\n    pub sale_price: f64,\n    pub sale_date: chrono::NaiveDate,\n    pub distance_miles: Option<f64>,\n    pub gla_sqft: Option<f64>,\n    pub beds: Option<i16>,\n    pub baths: Option<f64>,\n    pub year_built: Option<i32>,\n    pub data_source: Option<String>,\n    pub is_verified: bool,\n}\n\n#[derive(Debug, Serialize, Deserialize)]\npub struct ValuationInfo {\n    pub id: Uuid,\n    pub reconciled_value: f64,\n    pub low: f64,\n    pub high: f64,\n    pub sales_comparison_value: Option<f64>,\n    pub created_at: DateTime<Utc>,\n}"
