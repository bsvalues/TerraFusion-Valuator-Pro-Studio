-/// Calculate value range based on comp spread and statistical confidence
-fn calculate_value_range(reconciled_value: f64, analyses: &[CompAnalysisResult]) -> (f64, f64) {
-    if analyses.len() == 1 {
-        // Single comp: use Â±5% range
-        return (reconciled_value * 0.95, reconciled_value * 1.05);
-    }
-
-    // Calculate standard deviation of adjusted prices
-    let adjusted_prices: Vec<f64> = analyses.iter().map(|a| a.adjusted_price).collect();
-    let mean = adjusted_prices.iter().sum::<f64>() / adjusted_prices.len() as f64;
-    let variance = adjusted_prices
-        .iter()
-        .map(|price| (price - mean).powi(2))
-        .sum::<f64>()
-        / adjusted_prices.len() as f64;
-    let std_dev = variance.sqrt();
-
-    // Calculate coefficient of variation
-    let cv = std_dev / mean;
-
-    // Adjust range based on confidence level
-    let range_factor = if cv < 0.05 {
-        0.03 // High confidence: Â±3%
-    } else if cv < 0.10 {
-        0.05 // Medium confidence: Â±5%
-    } else {
-        0.08 // Lower confidence: Â±8%
-    };
-
-    let low = reconciled_value * (1.0 - range_factor);
-    let high = reconciled_value * (1.0 + range_factor);
-
-    (low, high)
-}
