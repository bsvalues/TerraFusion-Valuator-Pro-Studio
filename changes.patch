--- 0002_qc_enhancements.sql\n-- TerraFusion RiskSentinel QC Engine - Enhanced Quality Control Schema\n-- Created: 2025-11-14\n-- Description: Advanced QC rules, risk scoring, and AI-assisted quality control\n\n-- =============================================================================\n-- QC RULE DEFINITIONS\n-- =============================================================================\n\nCREATE TABLE qc_rule_definitions (\n    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    rule_code TEXT NOT NULL UNIQUE, -- QC001, QC002, etc.\n    rule_name TEXT NOT NULL,\n    category TEXT NOT NULL, -- data_quality, compliance, market_analysis, risk_assessment\n    severity_level TEXT NOT NULL, -- critical, high, medium, low, info\n    description TEXT NOT NULL,\n    \n    -- Rule Logic\n    rule_type TEXT NOT NULL, -- field_validation, comparison_check, ai_assisted, statistical_analysis\n    field_path TEXT, -- JSON path for field validation (e.g., \"subject_property.gla_sqft\")\n    validation_logic JSONB NOT NULL, -- Rule parameters and thresholds\n    \n    -- AI Integration\n    requires_ai_analysis BOOLEAN DEFAULT false,\n    ai_prompt_template TEXT, -- Template for AI analysis\n    \n    -- Rule Metadata\n    is_active BOOLEAN DEFAULT true,\n    auto_resolve BOOLEAN DEFAULT false, -- Can this rule be auto-resolved?\n    created_at TIMESTAMPTZ NOT NULL DEFAULT now(),\n    updated_at TIMESTAMPTZ NOT NULL DEFAULT now()\n);\n\nCREATE INDEX idx_qc_rule_definitions_category ON qc_rule_definitions(category);\nCREATE INDEX idx_qc_rule_definitions_severity ON qc_rule_definitions(severity_level);\nCREATE INDEX idx_qc_rule_definitions_active ON qc_rule_definitions(is_active);\n\n-- =============================================================================\n-- QC RULE EXECUTION LOG\n-- =============================================================================\n\nCREATE TABLE qc_rule_executions (\n    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    order_id UUID NOT NULL REFERENCES orders (id) ON DELETE CASCADE,\n    rule_id UUID NOT NULL REFERENCES qc_rule_definitions (id),\n    \n    -- Execution Details\n    execution_status TEXT NOT NULL, -- passed, failed, warning, error, pending_ai\n    execution_time_ms INTEGER, -- Performance tracking\n    \n    -- Results\n    finding_summary TEXT, -- Brief description of finding\n    detailed_analysis TEXT, -- Full analysis (especially for AI-generated)\n    confidence_score DOUBLE PRECISION, -- 0.0-1.0 confidence in the finding\n    \n    -- Data Context\n    evaluated_data JSONB, -- Snapshot of data that was evaluated\n    rule_parameters JSONB, -- Parameters used for this execution\n    \n    -- AI Analysis (if applicable)\n    ai_request_id TEXT, -- Track AI service requests\n    ai_response_raw TEXT, -- Raw AI response\n    ai_tokens_used INTEGER, -- Cost tracking\n    \n    -- Metadata\n    executed_at TIMESTAMPTZ NOT NULL DEFAULT now(),\n    executed_by TEXT DEFAULT 'system' -- system, user_id, or ai_agent_name\n);\n\nCREATE INDEX idx_qc_rule_executions_order_id ON qc_rule_executions(order_id);\nCREATE INDEX idx_qc_rule_executions_rule_id ON qc_rule_executions(rule_id);\nCREATE INDEX idx_qc_rule_executions_status ON qc_rule_executions(execution_status);\nCREATE INDEX idx_qc_rule_executions_executed_at ON qc_rule_executions(executed_at);\n\n-- =============================================================================\n-- ENHANCED QC ISSUES (Update existing table)\n-- =============================================================================\n\n-- Add new columns to existing qc_issues table\nALTER TABLE qc_issues ADD COLUMN IF NOT EXISTS rule_execution_id UUID REFERENCES qc_rule_executions (id);\nALTER TABLE qc_issues ADD COLUMN IF NOT EXISTS risk_score INTEGER DEFAULT 0; -- 0-100 risk score\nALTER TABLE qc_issues ADD COLUMN IF NOT EXISTS auto_generated BOOLEAN DEFAULT false;\nALTER TABLE qc_issues ADD COLUMN IF NOT EXISTS ai_suggested_resolution TEXT;\nALTER TABLE qc_issues ADD COLUMN IF NOT EXISTS impact_assessment TEXT; -- low, medium, high, critical\nALTER TABLE qc_issues ADD COLUMN IF NOT EXISTS requires_human_review BOOLEAN DEFAULT false;\n\n-- Add index for new columns\nCREATE INDEX IF NOT EXISTS idx_qc_issues_rule_execution ON qc_issues(rule_execution_id);\nCREATE INDEX IF NOT EXISTS idx_qc_issues_risk_score ON qc_issues(risk_score);\nCREATE INDEX IF NOT EXISTS idx_qc_issues_auto_generated ON qc_issues(auto_generated);\n\n-- =============================================================================\n-- QC ANALYSIS SESSIONS\n-- =============================================================================\n\nCREATE TABLE qc_analysis_sessions (\n    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    order_id UUID NOT NULL REFERENCES orders (id) ON DELETE CASCADE,\n    \n    -- Session Details\n    session_type TEXT NOT NULL, -- full_analysis, incremental, ai_review, manual_trigger\n    trigger_event TEXT, -- order_created, comps_added, valuation_completed, manual\n    \n    -- Results Summary\n    total_rules_executed INTEGER DEFAULT 0,\n    rules_passed INTEGER DEFAULT 0,\n    rules_failed INTEGER DEFAULT 0,\n    rules_warning INTEGER DEFAULT 0,\n    rules_error INTEGER DEFAULT 0,\n    \n    -- Risk Assessment\n    overall_risk_score INTEGER DEFAULT 0, -- 0-100\n    risk_level TEXT DEFAULT 'unknown', -- low, medium, high, critical\n    \n    -- Performance\n    execution_duration_ms INTEGER,\n    ai_requests_made INTEGER DEFAULT 0,\n    ai_tokens_consumed INTEGER DEFAULT 0,\n    \n    -- Status\n    status TEXT NOT NULL DEFAULT 'running', -- running, completed, failed, cancelled\n    completion_percentage INTEGER DEFAULT 0,\n    \n    -- Metadata\n    started_at TIMESTAMPTZ NOT NULL DEFAULT now(),\n    completed_at TIMESTAMPTZ,\n    triggered_by TEXT DEFAULT 'system' -- system, user_id, scheduled_job\n);\n\nCREATE INDEX idx_qc_analysis_sessions_order_id ON qc_analysis_sessions(order_id);\nCREATE INDEX idx_qc_analysis_sessions_status ON qc_analysis_sessions(status);\nCREATE INDEX idx_qc_analysis_sessions_started_at ON qc_analysis_sessions(started_at);\n\n-- =============================================================================\n-- AI AGENT INTERACTIONS\n-- =============================================================================\n\nCREATE TABLE ai_agent_interactions (\n    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    order_id UUID NOT NULL REFERENCES orders (id) ON DELETE CASCADE,\n    qc_session_id UUID REFERENCES qc_analysis_sessions (id),\n    \n    -- Agent Details\n    agent_name TEXT NOT NULL, -- RiskSentinel, CompHunter, NarrativeSmith, etc.\n    interaction_type TEXT NOT NULL, -- qc_analysis, comp_validation, narrative_review\n    \n    -- Request/Response\n    request_prompt TEXT NOT NULL,\n    response_content TEXT,\n    \n    -- Metadata\n    model_used TEXT, -- gpt-4, claude-3, etc.\n    tokens_requested INTEGER,\n    tokens_received INTEGER,\n    cost_estimate DECIMAL(10,4), -- Estimated cost in USD\n    \n    -- Performance\n    response_time_ms INTEGER,\n    confidence_score DOUBLE PRECISION, -- Agent's confidence in its response\n    \n    -- Status\n    status TEXT NOT NULL DEFAULT 'pending', -- pending, completed, failed, timeout\n    error_message TEXT,\n    \n    -- Timestamps\n    requested_at TIMESTAMPTZ NOT NULL DEFAULT now(),\n    completed_at TIMESTAMPTZ\n);\n\nCREATE INDEX idx_ai_agent_interactions_order_id ON ai_agent_interactions(order_id);\nCREATE INDEX idx_ai_agent_interactions_agent_name ON ai_agent_interactions(agent_name);\nCREATE INDEX idx_ai_agent_interactions_status ON ai_agent_interactions(status);\nCREATE INDEX idx_ai_agent_interactions_requested_at ON ai_agent_interactions(requested_at);\n\n-- =============================================================================\n-- INSERT DEFAULT QC RULES\n-- =============================================================================\n\n-- Data Quality Rules\nINSERT INTO qc_rule_definitions (rule_code, rule_name, category, severity_level, description, rule_type, field_path, validation_logic) VALUES\n('QC001', 'Subject GLA Required', 'data_quality', 'critical', 'Subject property must have GLA square footage', 'field_validation', 'subject_property.gla_sqft', '{\"required\": true, \"min_value\": 1}'),\n('QC002', 'Subject Beds/Baths Required', 'data_quality', 'high', 'Subject property must have bedroom and bathroom count', 'field_validation', 'subject_property', '{\"required_fields\": [\"beds\", \"baths\"]}'),\n('QC003', 'Comparable Sale Price Range', 'data_quality', 'medium', 'Comparable sale prices should be reasonable', 'field_validation', 'comparable_sales.sale_price', '{\"min_value\": 10000, \"max_value\": 10000000}'),\n('QC004', 'Comparable Sale Date Recency', 'market_analysis', 'high', 'Comparable sales should be recent (within 12 months)', 'comparison_check', 'comparable_sales.sale_date', '{\"max_age_days\": 365}'),\n('QC005', 'GLA Variance Check', 'market_analysis', 'medium', 'Comparable GLA should be similar to subject', 'statistical_analysis', 'comparable_sales.gla_sqft', '{\"max_variance_percent\": 25}');\n\n-- AI-Assisted Rules\nINSERT INTO qc_rule_definitions (rule_code, rule_name, category, severity_level, description, rule_type, validation_logic, requires_ai_analysis, ai_prompt_template) VALUES\n('QC101', 'Comparable Relevance Assessment', 'market_analysis', 'medium', 'AI assessment of comparable sale relevance', 'ai_assisted', '{\"min_comps\": 3}', true, 'Analyze the relevance of these comparable sales to the subject property. Consider location, size, age, and market conditions. Rate each comparable''s relevance on a scale of 1-10 and explain your reasoning.'),\n('QC102', 'Market Condition Analysis', 'market_analysis', 'low', 'AI analysis of current market conditions', 'ai_assisted', '{}', true, 'Based on the comparable sales data and sale dates, analyze the current market conditions in this area. Identify any trends, seasonal patterns, or market shifts that could affect the valuation.'),\n('QC103', 'Adjustment Logic Review', 'compliance', 'high', 'AI review of adjustment logic and reasonableness', 'ai_assisted', '{}', true, 'Review the adjustments made between the subject property and comparables. Assess whether the adjustments are reasonable, well-supported, and follow industry best practices.');\n\n-- Risk Assessment Rules  \nINSERT INTO qc_rule_definitions (rule_code, rule_name, category, severity_level, description, rule_type, validation_logic, requires_ai_analysis, ai_prompt_template) VALUES\n('QC201', 'High-Risk Transaction Detection', 'risk_assessment', 'critical', 'Detect potentially high-risk transactions', 'ai_assisted', '{}', true, 'Analyze this appraisal for potential risk factors including: unusual transaction circumstances, significant property condition issues, market volatility, or regulatory compliance concerns. Provide a risk assessment with specific recommendations.'),\n('QC202', 'Reviewer Kickback Probability', 'risk_assessment', 'medium', 'Predict likelihood of reviewer feedback', 'ai_assisted', '{}', true, 'Based on the appraisal data, comparable selections, and adjustments, predict the likelihood and type of feedback this appraisal might receive from an underwriter or reviewer. Suggest proactive improvements.');\n\n-- =============================================================================\n-- QC ANALYSIS VIEWS\n-- =============================================================================\n\n-- QC Summary by Order\nCREATE VIEW qc_order_summary AS\nSELECT \n    o.id as order_id,\n    o.client_name,\n    o.status as order_status,\n    sp.address,\n    \n    -- Latest QC Session\n    qs.id as latest_session_id,\n    qs.overall_risk_score,\n    qs.risk_level,\n    qs.status as qc_status,\n    qs.completed_at as last_qc_run,\n    \n    -- Issue Counts\n    COALESCE(issue_counts.total_issues, 0) as total_issues,\n    COALESCE(issue_counts.critical_issues, 0) as critical_issues,\n    COALESCE(issue_counts.high_issues, 0) as high_issues,\n    COALESCE(issue_counts.open_issues, 0) as open_issues,\n    \n    -- AI Usage\n    COALESCE(ai_stats.ai_interactions, 0) as ai_interactions_count,\n    COALESCE(ai_stats.total_tokens, 0) as total_ai_tokens\n    \nFROM orders o\nLEFT JOIN subject_properties sp ON o.id = sp.order_id\nLEFT JOIN LATERAL (\n    SELECT * FROM qc_analysis_sessions \n    WHERE order_id = o.id \n    ORDER BY started_at DESC \n    LIMIT 1\n) qs ON true\nLEFT JOIN LATERAL (\n    SELECT \n        COUNT(*) as total_issues,\n        COUNT(CASE WHEN severity = 'critical' THEN 1 END) as critical_issues,\n        COUNT(CASE WHEN severity = 'high' THEN 1 END) as high_issues,\n        COUNT(CASE WHEN status = 'open' THEN 1 END) as open_issues\n    FROM qc_issues \n    WHERE order_id = o.id\n) issue_counts ON true\nLEFT JOIN LATERAL (\n    SELECT \n        COUNT(*) as ai_interactions,\n        SUM(tokens_received) as total_tokens\n    FROM ai_agent_interactions \n    WHERE order_id = o.id\n) ai_stats ON true;\n\n-- QC Rule Performance\nCREATE VIEW qc_rule_performance AS\nSELECT \n    rd.rule_code,\n    rd.rule_name,\n    rd.category,\n    rd.severity_level,\n    \n    -- Execution Stats\n    COUNT(re.id) as total_executions,\n    COUNT(CASE WHEN re.execution_status = 'failed' THEN 1 END) as failure_count,\n    COUNT(CASE WHEN re.execution_status = 'warning' THEN 1 END) as warning_count,\n    \n    -- Performance\n    AVG(re.execution_time_ms) as avg_execution_time_ms,\n    AVG(re.confidence_score) as avg_confidence_score,\n    \n    -- AI Usage (if applicable)\n    COUNT(CASE WHEN re.ai_request_id IS NOT NULL THEN 1 END) as ai_requests,\n    AVG(re.ai_tokens_used) as avg_ai_tokens,\n    \n    -- Recent Activity\n    MAX(re.executed_at) as last_executed\n    \nFROM qc_rule_definitions rd\nLEFT JOIN qc_rule_executions re ON rd.id = re.rule_id\nWHERE rd.is_active = true\nGROUP BY rd.id, rd.rule_code, rd.rule_name, rd.category, rd.severity_level\nORDER BY rd.category, rd.severity_level;\n\n-- =============================================================================\n-- TRIGGERS FOR AUTOMATED QC\n-- =============================================================================\n\n-- Function to trigger QC analysis when valuation is completed\nCREATE OR REPLACE FUNCTION trigger_qc_analysis_on_valuation()\nRETURNS TRIGGER AS $$\nBEGIN\n    -- Insert a QC analysis session when a new valuation is created\n    INSERT INTO qc_analysis_sessions (order_id, session_type, trigger_event, triggered_by)\n    VALUES (NEW.order_id, 'incremental', 'valuation_completed', 'system');\n    \n    RETURN NEW;\nEND;\n$$ LANGUAGE plpgsql;\n\n-- Create trigger\nDROP TRIGGER IF EXISTS trigger_qc_on_valuation ON valuations;\nCREATE TRIGGER trigger_qc_on_valuation\n    AFTER INSERT ON valuations\n    FOR EACH ROW\n    EXECUTE FUNCTION trigger_qc_analysis_on_valuation();\n\n-- =============================================================================\n-- INDEXES FOR PERFORMANCE\n-- =============================================================================\n\n-- Composite indexes for common queries\nCREATE INDEX idx_qc_issues_order_severity ON qc_issues(order_id, severity, status);\nCREATE INDEX idx_qc_rule_executions_order_status ON qc_rule_executions(order_id, execution_status);\nCREATE INDEX idx_ai_interactions_order_agent ON ai_agent_interactions(order_id, agent_name, status);\n\nCOMMIT;"
